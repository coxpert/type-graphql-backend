{
    "name": "frontend-api",
    "version": "1.0.0",
    "main": "index.js",
    "license": "MIT",
    "scripts": {
        "start": "npm run build && node dist/server.js",
        "start:serverless": "serverless offline start --stage local --disableScheduledEvents",
        "dev": "cross-env NODE_ENV=development nodemon",
        "deploy:dev": "serverless deploy --stage dev",
        "deploy:prod": "serverless deploy --stage prod",
        "build": "tsc -w",
        "migrate": "knex migrate:latest --knexfile ./lib/db/knexfile.tx",
        "migrate:rollback": "knex migrate:rollback --knexfile ./lib/db/knexfile.tx",
        "seed": "knex seed:run --knexfile ./lib/db/knexfile.tx"
    },
